@model Auto3D.Web.ViewModels.PartnerLogos.PartnerLogoViewModel


@{ this.ViewData["Title"] = "Are you sure you want to delete the logo"; }
<div class="row justify-content-center">
    <form asp-area="Administration" asp-controller="PartnerLogos" asp-action="Delete" method="post" class="text-center col-md-4">
        <h4>@this.ViewData["Title"]</h4>
        <hr />
        <fieldset disabled="">
            <div class="form-group">
                @*<label asp-for="Logo"></label>*@
                <img style="width: 200px;" class="card-img-top" src="@Model.Path" alt="Partner logo" />
            </div>
        </fieldset>
            <button type="submit" class="btn btn-danger text-center mt-2">Delete Logo</button>
    </form>
</div>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}
